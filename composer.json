{
    "name": "sbutnariu/guzzle-header-forward-plugin",
    "type": "library",
    "description": "Guzzle plugin that allow you to forward headers from your current request into the the guzzle one automatically. Fork from neirda24/guzzle-bundle-header-forward-plugin adapted to work with Symfony 4.*",
    "keywords": [
        "middleware",
        "plugin",
        "framework",
        "http",
        "rest",
        "web service",
        "curl",
        "client",
        "HTTP client",
        "guzzle",
        "header",
        "forward"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Roches",
            "email": "adrien@roc-it.tech",
            "homepage": "https://github.com/Neirda24"
        },
        {
            "name": "Silviu Butnariu",
            "email": "silviubutnariu@yahoo.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.0",
        "eightpoints/guzzle-bundle": "~7.0",
        "symfony/http-kernel": "~4.0",
        "symfony/config": "~4.0",
        "symfony/dependency-injection": "~4.0",
        "symfony/expression-language": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1"
    },
    "autoload": {
        "psr-4": {
            "SilviuButnariu\\GuzzleHeaderForwardPlugin\\": "src"
        }
    }
}
